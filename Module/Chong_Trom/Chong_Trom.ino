
#define Led      A1
#define TinHieu  5
#define Chuong   2
/////////////////////////////////////////////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////////////////////////////
/////////////////////////////////////////////////////////////////////////////////////////
void setup(void) 
{
    Serial.begin(9600);
    pinMode(Chuong,  OUTPUT);
    pinMode(Led,     OUTPUT);
    pinMode(TinHieu, INPUT);
    ////////////////////
    analogWrite(Led, 150);
}
/////////////////////////////////////////////////////////////////////////////////////////


void loop(void) 
{
  digitalWrite(Chuong, HIGH);   // turn the LED on (HIGH is the voltage level)
  delay(1000);              // wait for a second
  digitalWrite(Chuong, LOW);    // turn the LED off by making the voltage LOW
  delay(1000);              // wait for a second

}

